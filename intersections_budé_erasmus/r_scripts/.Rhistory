library(ggplot2)
# set working directory
getwd()
setwd("../query_results/")
# read data
data<-read.csv("no_epp_per_modern_state_mut_corr_era_budé/no_epp_per_modern_state_sent_by_budé_to_mut_corr_era_budé_excl_era.csv", fileEncoding="UTF-8", na.strings=c("NULL"))
# create barchart
plot <- ggplot(data, aes(x= reorder(Name.Modern.State, -Number.of.letters.sent.to.this.modern.state.by.Budé.to.mutual.correspondents.of.his.and.Erasmus..excl..Erasmus.),y=Number.of.letters.sent.to.this.modern.state.by.Budé.to.mutual.correspondents.of.his.and.Erasmus..excl..Erasmus.)) +
