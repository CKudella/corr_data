# read data and define data type for date columns
data<-read.csv("no_corr_per_year/avg_no_epp_per_corr_year_to_era.csv", fileEncoding="UTF-8")
# create potblox
plot <- ggplot(data, aes(x= ' ', y = Average.number.of.letters.sent.per.correspondent.to.Erasmus.this.year)) +
geom_boxplot(outlier.size=2, notch = FALSE) +
geom_text(check_overlap = TRUE, aes(label=ifelse(Average.number.of.letters.sent.per.correspondent.to.Erasmus.this.year>=14.25,as.character(Location.Name),'')),hjust=-0.1,vjust=0) +
theme_bw() +
theme(axis.title.x=element_blank()) +
labs(y = "Average Number of letters sent from location by Erasmus per year")
