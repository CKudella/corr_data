library(ggplot2)
# set working directory
getwd()
setwd("../query_results/")
# read data
data<-read.csv("no_epp_per_year/no_epp_per_year_with_inferred_send_date_sent_by_pirck.csv", fileEncoding="UTF-8", na.strings=c("NULL"))
# create barchart
plot <- ggplot(data=data, aes(x=Year,y=Number.of.Letters.with.inferred.send.date.sent.by.Pirckheimer.this.year)) +
geom_bar(stat = "identity") +
geom_text(aes(label=Number.of.Letters.with.inferred.send.date.sent.by.Pirckheimer.this.year), vjust=-0.5, color='black', size = 2.5) +
labs(x="Year",y="Number of letters") +
scale_x_continuous(breaks = c(1484:1536)) +
scale_y_continuous(breaks = seq(0,240,10)) +
theme_bw() +
theme(axis.text.x = element_text(angle = 90, vjust = 0.35))
plot
